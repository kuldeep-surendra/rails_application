<div>
      <div class="container-fluid">
        
        <nav class="navbar navbar-inverse">
        <%= image_tag("logo.png",:class => "img_preview")%>
          <ul class="nav navbar-nav navbar-right">
            <li><%= link_to 'sign out',session_destroy_path%>
            </li>
            <!-- <li><button type="button" class="btn btn-success btn-md btn-space1" data-toggle="modal" data-target="#myModal1">Sign out</button>
            </li> -->
          </ul>
        </nav>
      </div>
    </div>


<div class = "col-sm-4">
    <div class="container">
    <%=form_for :event,url: event_create_form_path do |f|%>
          <%=f.label :Event_name%><br>
          <%=f.text_field :event_name%><br>
          <%=f.label :Venue%><br>
          <%=f.text_field :venue%><br>
          <%=f.label :Date%><br>
          <%=f.text_field :date%><br>
          <%=f.label :Time%><br>
          <%=f.text_field :time%><br>
          <%=f.label :Description%><br>
          <%=f.text_field :description%><br>
          <%=f.label :owner_id%><br>
          <%=f.text_field :owner_id,:value => current_user.id%><br>
          <%@users.each do |t|%><br>
          <%=check_box_tag 'invitee_ids[]',t.id%><%=t.username%><br>
          <%end%>
          <%=f.label :status%><br>
          <%=f.text_field :status%><br><br>
          <%=f.submit :ok%>
          <%= f.submit "cancel", :type => "reset" %><br>
        <%end%>
     </div>
     </div>